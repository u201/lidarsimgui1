        </rowConstraints>
         <children>
            <Label text="Beam Type" GridPane.rowIndex="1" />
            <ChoiceBox prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Axial Division" GridPane.rowIndex="2" />
            <Label text="Maximum order of scattering" GridPane.rowIndex="3" />
